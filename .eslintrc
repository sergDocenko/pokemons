{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "no-console": "warn",
    "quotes": ["warn", "double"],
    "max-len": ["warn", { "code": 100 }],
    "comma-dangle": ["warn", "always_multiline"],
    "semi": ["warn", "always"],
    "import/order": [
      "warn",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "object",
          "type"
        ],
        "newlines-between": "never"
      }
    ]
  }
}
